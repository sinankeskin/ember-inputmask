{
  "name": "ember-inputmask5",
  "version": "4.0.2",
  "description": "Ember addon for Inputmask input mask library.",
  "keywords": [
    "ember-addon",
    "inputmask",
    "mask"
  ],
  "repository": "https://github.com/sinankeskin/ember-inputmask",
  "license": "MIT",
  "author": "Sinan Keskin <sinan@kesk.in> (https://kesk.in)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup --config",
    "prepublishOnly": "rollup --config",
    "start": "rollup --config --watch",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:!'(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "test": "echo 'Addon does not have tests, run tests in test-app'",
    "prepare": "yarn build"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./test-support": "./dist/test-support/index.js",
    "./addon-main.js": "./addon-main.js"
  },
  "files": [
    "addon-main.js",
    "dist"
  ],
  "dependencies": {
    "@ember/render-modifiers": "^2.1.0",
    "@embroider/addon-shim": "^1.8.7",
    "ember-modifier": "^4.1.0",
    "inputmask": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@embroider/addon-dev": "^4.1.3",
    "@rollup/plugin-babel": "^6.0.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^11.0.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rollup": "^2.79.1"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "main": "addon-main.js",
    "type": "addon",
    "version": 2,
    "app-js": {
      "./components/inputmask.js": "./dist/_app_/components/inputmask.js",
      "./modifiers/inputmask.js": "./dist/_app_/modifiers/inputmask.js"
    }
  }
}
